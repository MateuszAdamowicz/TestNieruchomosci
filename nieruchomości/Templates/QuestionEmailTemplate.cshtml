@model Models.ContactEmail

@{
    Layout = null;
}
<body>
<div class="col-md-12">
    <h2>Użytkownik zapytał o ofertę!</h2>
    <b>Link do oferty:</b> @Model.OfferLink<br/>
    <hr/>
